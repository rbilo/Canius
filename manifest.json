{
    "manifest_version": 2, 
    "name": "Canius",
    "version": "1.0",
    "permissions": [
        "activeTab",
        "tabs",
        "<all_urls>"
    ],
    "description": "Removes the news articles from the Firefox homepage",
    "icons": {
        "48": "../canius/icons/icon48.png",
        "96": "../canius/icons/icon96.png",
        "128": "../canius/icons/icon.png"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                
                "canius.js"
            ]
        }
    ]
}